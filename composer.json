{
	"name": "aimeos/ai-ezpublish",
	"description": "Aimeos ezPublish integration",
	"keywords": ["aimeos", "extension", "ezpublish", "ezplatform", "adapter"],
	"homepage": "https://aimeos.org/eZPublish",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-ezpublish",
		"issues": "https://github.com/aimeos/ai-ezpublish/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.3",
		"doctrine/dbal": "~2.0",
		"composer/installers": "^1.0",
		"aimeos/aimeos-core": "~2016.10"
	},
	"require-dev": {
		"phing/phing": "~2.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\MShop\\": "lib/mshoplib/src/MShop/"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
